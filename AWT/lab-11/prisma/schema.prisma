generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id    Int     @id @default(autoincrement())
  name  String  @db.VarChar(100)
  email String  @unique @db.VarChar(150)
  city  String? @db.VarChar(100)
  pin   String? @db.Char(6)
}
